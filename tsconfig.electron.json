{
   "include": ["electron"],
   "compilerOptions": {
      "composite": true,
      "types": ["node"],
      "target": "ESNext",
      "module": "commonjs",
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "noImplicitAny": true,
      "sourceMap": true,
      "baseUrl": ".",
      "outDir": "dist",
      "moduleResolution": "node",
      "resolveJsonModule": true
   }
}
